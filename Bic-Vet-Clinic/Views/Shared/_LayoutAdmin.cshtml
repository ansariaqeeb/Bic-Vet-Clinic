<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <link href="~/faviconc9 .ico" rel="shortcut icon" type="image/x-icon" />


    <title>@ViewBag.Title</title>

    @System.Web.Optimization.Styles.Render("~/bundles/LayoutCommoncss")
    @System.Web.Optimization.Scripts.Render("~/bundles/LayoutJS") 
</head>
 
<body class="skin-2" ondrag="return false;" ondrop="return false;">
    <div id="navbar" class="navbar navbar-default" style="background-color: @System.Configuration.ConfigurationManager.AppSettings["ThemeColor"];">
        <script type="text/javascript">
            try { ace.settings.check('navbar', 'fixed') } catch (e) { }
        </script>
        @{
            DataModel.LoginModel.LoginSessionDetails SessLogObj = new DataModel.LoginModel.LoginSessionDetails();
            SessLogObj = (DataModel.LoginModel.LoginSessionDetails)Session["SessionInformation"];
        }
        <div class="navbar-container" id="navbar-container">
            <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler" data-target="#sidebar">
                <span class="sr-only">Toggle sidebar</span>

                <span class="icon-bar"></span>

                <span class="icon-bar"></span>

                <span class="icon-bar"></span>
            </button>

            <!-- /section:basics/sidebar.mobile.toggle -->
            <div class="navbar-header pull-left">
                <!-- #section:basics/navbar.layout.brand -->
                <a href="#" class="navbar-brand no-padding">
                    @*<img src="~/Content/Images/applogotrans.png" />*@
                </a>
            </div>

            <!-- #section:basics/navbar.dropdown -->
            <div class="navbar-buttons navbar-header pull-right" role="navigation">
                <ul class="nav ace-nav">

                    <li class="purple2">
                        <a data-toggle="dropdown" href="#" class="dropdown-toggle" title="" style="">
                            <img class="nav-user-photo" src="~/Content/Images/avatar2.png" alt="@SessLogObj.objLoginM.userName" />

                            <span class="user-info">
                                <small>Welcome,</small>@SessLogObj.objLoginM.userName
                            </span>

                            <i class="ace-icon fa fa-caret-down"></i>
                        </a>
                        <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                            <li>
                                <a href="/Account/DefLogOff">
                                    <i class="ace-icon fa fa-power-off"></i>
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </li>

                    <!-- /section:basics/navbar.user_menu -->
                </ul>
            </div>

            <!-- /section:basics/navbar.dropdown -->
        </div><!-- /.navbar-container -->
    </div>

    <!-- /section:basics/navbar.layout -->
    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>

        <!-- #section:basics/sidebar -->
        @if (System.Configuration.ConfigurationManager.AppSettings["HideSidebar"] == "0")
        {
            <div id="sidebar" class="sidebar responsiv">
                <script type="text/javascript">
                    try {
                        ace.settings.check('sidebar', 'fixed')
                    } catch (e) { }
                </script>
                @{
                    string HideMenuGroupStrip = System.Configuration.ConfigurationManager.AppSettings["HideMenuGroupStrip"] == null ? "0" : System.Configuration.ConfigurationManager.AppSettings["HideMenuGroupStrip"].ToString();
                    if (HideMenuGroupStrip == "0")
                    {
                        <div class="sidebar-shortcuts hide" id="sidebar-shortcuts">

                            <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
                                <a id="tooltip4" data-placement="bottom" data-rel="tooltip" data-original-title="Dashboard" class="btn btn-success" href='#'>
                                    <i class="ace-icon fa fa-dashboard"></i>
                                </a>

                                <a id="tooltip1" data-placement="bottom" data-rel="tooltip" data-original-title="Home" class="btn btn-info" href='@(Url.Action("Index", "Home"))'>
                                    <i class="ace-icon fa fa-home"></i>
                                </a>

                                <!-- #section:basics/sidebar.layout.shortcuts -->
                                <a id="tooltip2" data-placement="bottom" data-rel="tooltip" data-original-title="Report" class="btn btn-warning" href='#'>
                                    @*href='@(Url.Action("Index", "BI", new { Id = "507" }))'*@
                                    <i class="ace-icon fa fa-signal"></i>
                                </a>

                                <a id="tooltip3" data-placement="bottom" data-rel="tooltip" data-original-title="Master" class="btn btn-danger" href='#'>
                                    <i class="ace-icon fa fa-th"></i>
                                </a>

                                <!-- /section:basics/sidebar.layout.shortcuts -->
                            </div>

                            <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
                                <span class="btn btn-success"></span>
                                <span class="btn btn-info"></span>
                                <span class="btn btn-warning"></span>
                                <span class="btn btn-danger"></span>
                            </div>
                        </div>
                    }
                }
                <!-- /.sidebar-shortcuts -->
                <div id="nav-search" class="nav-search hide">
                    <form class="form-search">
                        <span class="input-icon align-left searchtxt-input">
                            <input type="text" autocomplete="off" id="searchtxt" class="nav-search-input " placeholder="Search">
                            <ul class="typeahead dropdown-menu" style="top: 28px; left: 0px; display: none;">
                                <li data-value="South Carolina" class="active"><a href="#"><strong>S</strong>outh Carolina</a></li>
                                <li data-value="South Dakota" class=""><a href="#"><strong>S</strong>outh Dakota</a></li>
                                <li data-value="Alaska" class=""><a href="#">Ala<strong>s</strong>ka</a></li>
                                <li data-value="Arkansas" class=""><a href="#">Arkan<strong>s</strong>a<strong>s</strong></a></li>
                                <li data-value="Illinois" class=""><a href="#">Illinoi<strong>s</strong></a></li>
                                <li data-value="Kansas"><a href="#">Kan<strong>s</strong>a<strong>s</strong></a></li>
                                <li data-value="Louisiana"><a href="#">Loui<strong>s</strong>iana</a></li>
                                <li data-value="Massachusetts"><a href="#">Ma<strong>s</strong><strong>s</strong>achu<strong>s</strong>ett<strong>s</strong></a></li>
                            </ul>
                            <i class="ace-icon fa fa-search nav-search-icon"></i>
                        </span>
                        @{
                            string ShowHomePage = System.Configuration.ConfigurationManager.AppSettings["HomePage"] == null ? "0" : System.Configuration.ConfigurationManager.AppSettings["HomePage"].ToString();
                        }
                        @if (ShowHomePage == "1")
                        {
                            <a id="IdHomePage" href="@(Url.Action("Index", "Home"))" style="color:black;">
                                <i class="fa fa-home bigger-150" style="cursor:pointer;"></i>
                            </a>
                        }
                    </form>
                </div>
                <div id="DivwaitingLayout" class="hide widget-box-overlay clsoverlay">
                    <div class="center">
                        <i class="fa-spinner fa fa-spin orange bigger-250" style="margin-top:20%"></i>
                    </div>
                </div>
                <div id="menu" class="hide">
                    @*Code for loading menus*@
                    @*@Html.Action("Menu", "Home", new { id = 0 })*@
                </div>
                <!-- /.nav-list -->
                <!-- #section:basics/sidebar.layout.minimize -->
                <div class="sidebar-toggle sidebar-collapse" id="sidebar-collapse">
                    <i class="ace-icon fa fa-angle-double-left" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i>
                </div>
                <!-- /section:basics/sidebar.layout.minimize -->
                <script type="text/javascript">
                    try {
                        ace.settings.check('sidebar', 'collapsed')
                    }
                    catch (e) { }

                    $("#sidebar-collapse").click(function () {
                        $("#IdHomePage").toggleClass('hide');
                    });
                </script>
            </div>
        }
        <!-- /section:basics/sidebar -->
        <div class="main-content">
            <div class="main-content-inner">
                <div class="breadcrumbs no-margin no-padding-right" id="newBread">
                    <script type="text/javascript">
                        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
                    </script>
                    @if (System.Configuration.ConfigurationManager.AppSettings["ThemeColor"] != "")
                    {
                        <script type="text/javascript">
                            $(document).ready(function () {
                                $("#newBread ul li h4").removeClass('purple');
                                $("#newBread ul li h4").css('color', '@System.Configuration.ConfigurationManager.AppSettings["ThemeColor"] !important');
                            });
                        </script>
                    }
                    <ul class="breadcrumb menu-name">
                        <li>
                            <h4 class="purple" style="text-shadow : 1px 1px 1px rgb(213, 213, 213);" id="Menutext"></h4>
                        </li>
                    </ul>
                </div>

                @*style="background-color: @System.Configuration.ConfigurationManager.AppSettings["ThemeColor"];"*@
                <!-- /section:basics/content.breadcrumbs -->
                <div id="page-content" class="page-content">
                    <div class="row">
                        @*Main body for rendering views*@
                        @RenderBody()
                    </div><!-- /.row -->
                </div><!-- /.page-content -->
            </div>
        </div><!-- /.main-content -->

        <div class="footer">
            <div class="footer-inner">
                <div class="footer-content">
                    <span>
                        <a href="https://www.bicsolutionsltd.com/" target="_blank"><span class="blue bolder ">B.I.C Solutions Ltd.</span></a>
                        Your partner to your complete business solution.&copy; 2019-2020
                    </span>

                    &nbsp; &nbsp;
                    <span class="action-buttons">

                        <a href="https://www.facebook.com/bicsolutionsltd" target="_blank">
                            <i class="ace-icon fa fa-facebook-square light-blue bigger-150"></i>
                        </a>

                        <a href="https://www.linkedin.com/company/bicsolutionsltd" target="_blank">
                            <i class="ace-icon fa fa-linkedin-square light-blue bigger-150"></i>
                        </a>
                    </span>
                </div>
            </div>
        </div>

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>

        @*Chat Model added by archana*@
        <div id="right-menu" class="modal aside aside-right aside-vc aside-fixed navbar-offset aside-hidden no-backdrop" data-backdrop="false" data-body-scroll="false" data-offset="true" data-placement="right" data-fixed="true" @* tabindex="-1" *@ style="display: block; position: fixed; padding-right: 17px;">
            <div class="modal-dialog">
                <div class="modal-content ace-scroll" style="background-color:white;">
                    @*<div class="scroll-track scroll-dark no-track idle-hide scroll-active" style="display: block; height: 303px;"><div class="scroll-bar" style="height: 85px; top: 0px;"></div></div><div class="scroll-content" style="max-height: 303px;">*@
                    <div class="modal-header no-padding">
                        <div class="table-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                <span class="white">×</span>
                            </button>
                            Online Users
                        </div>
                    </div>
                    <div id="ModalOLUser" class="modal-body">

                    </div>
                </div>
            </div>
        </div><!-- /.modal-dialog  -->
        <div class="panel panel-default pull-right no-padding no-margin chatdv" style="position:fixed;bottom:0;right:16%;z-index:999;width:240px;">

        </div>
        <div id="DivwaitingCommonInfoPartial" class="hide widget-box-overlay">
            <div class="center">
                <i class="fa-spinner fa fa-spin orange bigger-250" style="margin-top:20%"></i>
            </div>
        </div>
        @*Common Info Partial -- start*@
        <div id="divCommonInfoPartial" tabindex="-1" class="modal fade" role="dialog" aria-labelledby="divCommonInfoPartialModal" data-backdrop="static" data-keyboard="false" aria-hidden="true">
            <div id="" class="modal-dialog modal-lg">
                <div class="modal-content col-sm-12">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            &times;
                        </button>
                        <h4 class="modal-title" id="HeaderCommonInfo"></h4>
                    </div>
                    <div id="DivLoadCommonInfo" class="modal-body row no-padding">
                    </div>
                </div>
            </div>
        </div>
        @*Common Info Partial -- end*@
    </div>

    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.js'>" + "<" + "/script>");


        (function ($) {

            $("#sidebar-collapse").click();
            $("#sidebar-collapse").addClass('hide');
            //Chat sidebar
            $('.aside').ace_aside({
                placement: 'right',
                offset: true,
                background: true,
                body_scroll: false
            });



            if ('@TempData["RESULTID"]' != null && '@TempData["RESULTID"]' != "") {
                var data = '@TempData["MESSAGE"]';

                $.c9Message('@TempData["MESSAGE"]', '@TempData["TITLE"]', '@TempData["TYPE"]');
            }

            $('#searchtxt').typeahead({
                ajax: {
                    url: '/Home/menusearch?term=',
                    triggerLength: 1,
                    preDispatch: function (qry) {
                        return { term: qry };
                    }
                },
                display: 'value',
                val: 'url',
                itemSelected: function (item, ele) {
                    var i = ele.indexOf("/LookUp/LookupResult");
                    //debugger; This code for loading spinner   by MAksud 14Jan2019
                    $('#DivwaitingLayout').removeClass('hide');
                    // $('#DivwaitingLayout').addClass('hide');
                    window.location.href = ele;
                }
            });


            //this fun call for validate input copy-paste value for filed like c9-type="numeric"
            $.c9CheckInputType();


        }(jQuery));



        function RemoveSelectWarning(id) {
            $('#s2id_' + id).find('.select2-choice').removeClass('clsShowWarning');
        }
        function RemoveSelectError(id) {
            $('#s2id_' + id).find('.select2-choice').removeClass('clsShowError');
        }



    </script>
</body>


</html>
